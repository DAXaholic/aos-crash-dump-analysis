<?xml version="1.0"?>
<configuration>
    <options log='true' retention_period='P1M' />
    <!-- 

     Example tag for specifying a dump source. It is possible to
     specify multiple 'dump_source' tags.

     Attributes:
        - source_path -> Path to watch for new dumps.
        - move_path   -> Path to which the analysis log and dumps are moved to.

    <dump_source source_path='\\MyAOS\...\DebugDiag\Logs\Ax32Serv.exe\'
                   move_path='\\MyNetShare\...\AOSCrashDumpAnalysis\'/>

    -->
</configuration>
